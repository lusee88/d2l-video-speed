{
    "name": "Getting Started Example",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/fast_forward16.png",
            "32": "/images/fast_forward32.png",
            "48": "/images/fast_forward48.png",
            "128": "/images/fast_forward128.png"
        }
    },
    "icons": {
        "16": "/images/fast_forward16.png",
        "32": "/images/fast_forward32.png",
        "48": "/images/fast_forward48.png",
        "128": "/images/fast_forward128.png"
      }
}

